{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "config/*": [
        "src/config/*"
      ],
      "pages/*": [
        "src/pages/*"
      ]
    },
    "types": ["@mui/material", "@emotion/react/types/css-prop"]
  },
  "include": [
    "src",
    "node_modules/@mui/material",
    "node_modules/@emotion/react"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}